{
    "languageserver": {
        "tailwindCSS.headwind.sortTailwindClasses": "off",
        "golang": {

            "command": "gopls",
            "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
            "filetypes": ["go"],
            "initializationOptions": {
                "usePlaceholders": true
            }
        }
    },
    "go.goplsEnv": {
        "GOOS": "js",
        "GOARCH": "wasm"
    },
    "tslint.autoFixOnSave": true
}

